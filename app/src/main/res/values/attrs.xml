<resources>
    <declare-styleable name="ExpandableLayout">
        <attr name="canExpand" format="boolean" />
        <attr name="startExpanded" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="FontStyle">
        <attr name="font" format="string" />
        <attr name="fontType" />
    </declare-styleable>

    <attr name="fontType" format="enum">
        <enum name="light" value="0"></enum>
        <enum name="medium" value="1"></enum>
        <enum name="bold" value="2"></enum>
        <enum name="openSansSemibold" value="3"></enum>
        <enum name="openSansLight" value="4"></enum>
    </attr>

    <!--Circle animation-->

    <declare-styleable name="TimerView">
        <attr name="circleColor" format="color" />
    </declare-styleable>

    <declare-styleable name="MaskedEditText">
        <attr name="mask" format="string" />
        <attr name="placeholder" format="string" />
    </declare-styleable>


</resources>

